[user]
    name = Billy Keyes
    email = bluekeyes@gmail.com

[core]
    editor = "vim"

[color]
    ui = auto
    branch = auto
    diff = auto
    interactive = auto
    status = auto

[alias]
    st = status -uall
    br = branch
    co = checkout
    pk = cherry-pick -xe
    df = diff
    dfc = diff --cached
    amend = commit --amend
    l = log
    ln = log --name-only
    lp = log -p
    ff = merge --ff-only
