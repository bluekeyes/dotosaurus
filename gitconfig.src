[user]
    name = {{CUSTOM}}
    email = {{CUSTOM}}

[core]
    editor = "vim"

[color]
    ui = auto
    branch = auto
    diff = auto
    interactive = auto
    status = auto

[alias]
    st = status -uall
    br = branch
    co = checkout
    pk = cherry-pick -xe
    df = diff
    dfc = diff --cached
